@model AddEditVendorViewModel

@{
    ViewData["Title"] = "Edit vendor";
}

<form asp-controller="VendorInvoice" asp-action="ProcessEditRequest" method="post" enctype="application/x-www-form-urlencoded">
    <input type="hidden" asp-for="Vendor.VendorId" />
    <partial name="_VendorFormsPartial" model="Model" />
    <button type="submit" class="btn btn-primary">Save</button>
</form>

@section scripts {
    <partial name="_ValidationScriptsPartial" />
}